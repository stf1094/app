/**
 * 	Style for The User Template
 */
".template": {
	"height": 100,
	"width": Ti.UI.FILL
},
".wrapper": {
	"backgroundColor": "#fff",
	"orientationModes": [Ti.UI.PORTRAIT,Ti.UI.LANDSCAPE_LEFT,Ti.UI.LANDSCAPE_RIGHT,Ti.UI.UPSIDE_PORTRAIT],
	"exitOnClose": true,
	"titleAttributes": {color:"#ae331f"},
	"hideShadow": true,
	"borderColor": "#fff",
	"borderWidth": 1,
	"translucent": true,
	"extendEdges": Titanium.UI.EXTEND_EDGE_TOP
},
".wrapper[platform=ios]": {
	"navTintColor": "#ae331f"
},
".logo": {
	"text": "\f2b9",
	"color": "#C41230",
	"font": {fontFamily:"fontawesome",fontSize:36}
},
".favorite": {
	"font": {fontSize:25,fontFamily:"icomoon",},
	"color": "#038BC8",
	"text": "\uf005",
	"right": 10
},
".title": {
	"font": {fontSize:14},
	"left": 85,
	"color": "#444"
},
".subtitle": {
	"font": {fontSize:10},
	"left": 85,
	"height": 20,
	"color": "#666"
},
".avatar": {
	"borderWidth": 1,
	"borderColor": "#acacac",
	"height": 75,
	"width": 75,
	"top": 12,
	"left": 0,
	"borderRadius": 35,
	"defaultImage": "/images/profile_default/default_profile_image.jpg"
},
".smoothAvatarBorderRadius": {
	"borderWidth": 1.5,
	"borderColor": "#acacac",
	"height": 75,
	"width": 75,
	"top": 12,
	"left": 0,
	"borderRadius": 35,
	"opacity": 0.6
},
".search": {
	"backgroundColor": "#fff",
	"borderWidth": 0,
	"borderColor": "#fff",
	"tintColor": "#C41230",
	"hintText": "Search",
	"barColor": "#fff",
	"showBookmark": true
},
".icon-search[platform=android,windows]": {
	"font": {fontFamily:"icomoon",fontSize:24,},
	"text": "\uf002",
	"color": "#999",
	"width": "7%"
},
".icon-close[platform=android,windows]": {
	"font": {fontFamily:"icomoon",fontSize:24,},
	"text": "\uf057",
	"color": "#999",
	"height": 40,
	"width": "7%",
	"visible": false
},
"#searchBar[platform=android,windows]": {
	"height": 40,
	"left": 10,
	"width": "76%"
},
"#listView": {
	"tintColor": "#666",
	"backgroundColor": "#fff"
},
"#btnBack": {
},
"#wrapper": {
}